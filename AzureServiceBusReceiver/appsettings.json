{
	"ServiceBus": {
		"ServiceBusConnectionString": "",
		"QueueName": "poc",
		"MaxConcurrentCalls": 100,
		"PrefetchCount": 0
	},
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console" ],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Information",
				"System": "Information"
			}
		},
		"WriteTo:Async": {
			"Name": "Async",
			"Args": {
				"configure": [
					{
						"Name": "Console"
					},
					{
						"Name": "Seq",
						"Args": { "serverUrl": "http://localhost:5341" }
					}
				]
			}
		}
	}
}
